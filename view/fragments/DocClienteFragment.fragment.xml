                                                    </items>
                                                </Select>
                                            </f:fields>
                                        </f:FormElement>
                                        <f:FormElement label="Canal">
                                            <f:fields>
                                                <Select width="100%" selectedKey="{Country}">
                                                    <items>
                                                        <core:Item text="Germany" key="Germany" />
                                                        <core:Item text="USA" key="USA" />
                                                        <core:Item text="England" key="England" />
                                                    </items>
                                                </Select>
                                            </f:fields>
                                        </f:FormElement>
